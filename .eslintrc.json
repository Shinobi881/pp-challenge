{
  "extends": ["semistandard", "standard-react"],
  "rules": {
    "no-unused-vars": [0],
    "prefer-const": [0],
    "global-require": [0],
    "comma-dangle": [0],
    "no-console": [0],
    "no-case-declarations": [0],
    "arrow-body-style": [0],
    "array-callback-return": [0],
    "consistent-return": [0],
    "no-useless-constructor": [0],
    "space-before-function-paren": [0],
    "no-debugger": [0]
  }
}
